<%- include('../partials/header') %>

<h2>New Ticket</h2>
<form action="/tickets" method="POST">
    <label>Seat:
        <input name="seat" required pattern="[A-F][1-9]\d?">
    </label><br>
    <label>Price:
        <input type="number" name="price">
    </label><br>
  <button type="submit">Buy Ticket</button>
</form>